{
  "comments": {
    "blockComment": ["#", "#"]
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["@条件", "@效果"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")", "notIn": ["string", "comment"] },
    { "open": "[", "close": "]", "notIn": ["string", "comment"] },
    { "open": "{", "close": "}", "notIn": ["string", "comment"] },
    { "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
  ],
  "surroundingPairs": [
    ["#", "#"],
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["\"", "\""]
  ]
}
